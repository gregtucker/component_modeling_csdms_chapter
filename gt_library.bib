	issn = {{0169-555X}},
	journal = {{GEOMORPHOLOGY}},
	month = {{AUG 30}},
	number = {{3-4}},
	pages = {{222-235}},
	title = {{The influence of landsliding on sediment supply and channel change in a steep mountain catchment}},
